{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "europe-west1",
      "nextjs": {
        "source": "src",
        "functionConfig": {
          "memory": "512MB",
          "timeoutSeconds": 120
        }
      }
    }
  },
  "remoteconfig": {
    "template": "remoteconfig.template.json"
  },
  "extensions": {}
}
